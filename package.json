    "prepare": "npm run build",
    "clean": "rimraf dist"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.0",
    "axios": "^1.6.7",
    "bs58": "^6.0.0",
    "tweetnacl": "^1.0.3",
    "viem": "^2.23.9",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  }
}
